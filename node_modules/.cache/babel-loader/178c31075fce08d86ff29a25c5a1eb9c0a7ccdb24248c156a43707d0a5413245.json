n'\">\r\n      <form @submit.prevent=\"login\">\r\n        <h2 class=\"form__title\">登录账号</h2>\r\n        <input v-model=\"signinForm.account\" type=\"text\" placeholder=\"用户名/手机号\" class=\"input\" />\r\n        <input v-model=\"signinForm.password\" type=\"password\" placeholder=\"密码\" class=\"input\" />\r\n        <a href=\"#\" class=\"link\" @click.prevent=\"forgetPassword\">忘记密码?</a>\r\n        <button class=\"btn\" type=\"submit\">登 录</button>\r\n      </form>\r\n    </div>\r\n\r\n    <!-- Overlay -->\r\n    <div class=\"container__overlay\">\r\n      <div class=\"overlay\">\r\n        <div class=\"overlay__panel overlay--left\" @click=\"switchPanel('signin')\">\r\n          <button class=\"btn\">登录账号</button>\r\n        </div>\r\n        <div class=\"overlay__panel overlay--right\" @click=\"switchPanel('signup')\">\r\n          <button class=\"btn\">注册账号</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'AuthForm',\r\n  data() {\r\n    return {\r\n      activePanel: 'signin', // 默认显示登录面板\r\n      signupForm: {\r\n        phone: '',\r\n        password: '',\r\n        verificationCode: ''\r\n      },\r\n      signinForm: {\r\n        account: '',\r\n        password: ''\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    switchPanel(panel) {\r\n      this.activePanel = panel;\r\n    },\r\n    register() {\r\n      // 注册逻辑\r\n      console.log('注册', this.signupForm);\r\n    },\r\n    login() {\r\n      // 登录逻辑\r\n      console.log('登录', this.signinForm);\r\n    },\r\n    getVerificationCode() {\r\n      // 获取验证码逻辑\r\n      console.log('获取验证码');\r\n    },\r\n    forgetPassword() {\r\n      // 忘记密码逻辑\r\n      console.log('忘记密码');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 在这里添加你的CSS样式 */\r\n</style>"],"mappings":"AAwCA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,QAAQ;MAAE;MACvBC,UAAU,EAAE;QACVC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZ