nt=\"login\">\r\n        <h2 class=\"form__title\">登录账号</h2>\r\n        <input type=\"text\" v-model=\"accountName\" placeholder=\"用户名/手机号\" class=\"input\" />\r\n        <input type=\"password\" v-model=\"loginPassword\" placeholder=\"密码\" class=\"input\" />\r\n        <a href=\"#\" class=\"link\" @click.prevent=\"forgetPassword\">忘记密码?</a>\r\n        <button class=\"btn\" type=\"submit\">登 录</button>\r\n      </form>\r\n    </div>\r\n\r\n    <!-- Overlay -->\r\n    <div class=\"container__overlay\">\r\n      <div class=\"overlay\">\r\n        <div class=\"overlay__panel overlay--left\">\r\n          <button class=\"btn\" @click=\"signIn\">登录账号</button>\r\n        </div>\r\n        <div class=\"overlay__panel overlay--right\">\r\n          <button class=\"btn\" @click=\"signUp\">注册账号</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    </div>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      phoneNumber: '',\r\n      password: '',\r\n      verificationCode: '',\r\n      accountName: '',\r\n      loginPassword: '',\r\n      showSignUp: false\r\n    };\r\n  },\r\n  methods: {\r\n    signIn() {\r\n      this.showSignUp = false;\r\n    },\r\n    signUp() {\r\n      this.showSignUp = true;\r\n    },\r\n    getVerificationCode() {\r\n      // 实现获取验证码的逻辑\r\n      alert('验证码已发送，请查收！');\r\n    },\r\n    login() {\r\n      // 实现登录逻辑\r\n      this.$router.push({ name: 'Home' });\r\n    },\r\n    register() {\r\n      // 实现注册逻辑\r\n      alert('注册成功！');\r\n      this.$router.push({ name: 'Home' });\r\n    },\r\n    forgetPassword() {\r\n      this.$router.push({ name: 'ForgetPassword' });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n:root {\r\n  /* COLORS */\r\n  --white: #F5F7FF;\r\n  --gray: #333;\r\n  --blue: #3161FF;\r\n  --lightblue: #3161F2;\r\n\r\n  /* RADII */\r\n  --button-radius: 0.7rem;\r\n\r\n  /* SIZES */\r\n  --max-width: 700px;\r\n  --max-height: 373px;\r\n\r\n  font-size: 16px;