ition: fixed;\r\n  background-color: #3131FF;\r\n  color: white;\r\n  padding: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n</style>"],"mappings":";;AAcA,OAAOA,gBAAe,MAAO,wBAAwB;AACrD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,MAAM,EAAE;;AAEjC,eAAe;EACbC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,gBAAgB,EAAE,EAAE;MACpBC,wBAAwB,EAAE;IAC5B,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACT,GAAGL,QAAQ,CAAC;MACTM,WAAW,EAAEC,KAAI,IAAKA,KAAK,CAACC,QAAQ,CAACF,WAAU,CAAE;IACnD,CAAC;EACH,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACC,kCAAkC,CAAC,CAAC;EAC3C,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,kCAAkCA,CAAA,EAAG;MACzC,IAAI;QACF,MAAME,QAAO,GAAI,MAAMb,KAAK,CAACc,GAAG,CAAC,8BAA8B,IAAI,CAACP,WAAW,EAAE,CAAC;QAClF,IAAI,CAACH,gBAAe,GAAIS,QAAQ,CAACV,IAAI;QACrC,IAAI,CAACY,sBAAsB,CAAC,CAAC;MAC/B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,mDAAmD,EAAEA,KAAK,CAAC;MAC3E;IACF,CAAC;IACDD,sBAAsBA,CAAA,EAAG;MACvB,IAAI,CAACV,wBAAuB,GAAI,IAAI,CAACD,gBAAgB,CAACc,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACZ,WAAU,KAAM,IAAI,CAACA,WAAW,CAAC;IAC7G;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}